{
  "name": "@minh1509sotatek/auth_service",
  "version": "1.0.0",
  "private": true,
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "prepare": "husky",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:gen": "typeorm-ts-node-commonjs migration:generate -d ormconfig",
    "migrate:create": "typeorm migration:create",
    "migrate:run": "typeorm-ts-node-commonjs migration:run -d ormconfig",
    "migrate:prod": "typeorm migration:run -d dist/ormconfig.js",
    "migrate:revert": "typeorm-ts-node-commonjs migration:revert -d ormconfig",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js src/config -r ormconfig.ts",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -r ormconfig.ts",
    "seed:runOne": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -r ormconfig.ts -s",
    "template:install": "yarn workspace @repo/email-templates install",
    "template:dev": "yarn workspace @repo/email-templates dev",
    "template:build": "yarn workspace @repo/email-templates build",
    "template:build-docusign": "yarn workspace @repo/email-templates docusign",
    "command:run": "node dist/src/cli",
    "master-data:create-country": "node dist/src/cli create-country",
    "master-data:migrate-user-role": "node dist/src/cli migrate-user-role",
    "master-data:create-role-access-module": "node dist/src/cli create-role-access-module",
    "master-data:create-field-permission": "node dist/src/cli create-field-permission"
  },
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.896.0",
    "@aws-sdk/lib-storage": "^3.896.0",
    "@aws-sdk/s3-request-presigner": "^3.896.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/passport-jwt": "^4.0.1",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.9",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "chalk": "^5.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "google-auth-library": "^9.15.1",
    "handlebars": "^4.7.8",
    "husky": "^9.1.7",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "multer": "^2.0.1",
    "nest-commander": "^3.17.0",
    "nest-winston": "^1.10.2",
    "nestjs-i18n": "^10.5.1",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "pg": "^8.15.6",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "typeorm": "^0.3.22",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^11.1.0",
    "validatorjs": "^3.22.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.8",
    "@types/amqplib": "^0.10.7",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@types/validatorjs": "^3.15.5",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@swc/core"
    ]
  }
}
